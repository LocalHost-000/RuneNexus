{%  extends 'layout.twig' %}

{% block content %}

<div class="container" style="margin-top:-40px;">
    {% include 'commands/components/header.twig' %}

    <div class="row mb-3">
        <div class="col-sm-12 col-lg-2">

            {% include 'commands/components/sidebar_nav.twig' %}
            

        </div>
        <div class="col-sm-12 col-lg-10">
           

          <table class="table">
            <thead>
              <tr>
                <th scope="col">Type</th>
                <th scope="col">Command</th>
                <th scope="col">Usage</th>
                <th scope="col">Info</th>
                <th scope="col">Code</th>
              </tr>
            </thead>
            <tbody>

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Owner</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::master</span></th>
                <td><span class="badge bg-warning text-white">::master</span></td>
                <td>Gives player all 99 skills</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandMaster">
                    Code
                  </button>
              </td>
              </tr>

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Owner</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::pickup</span></th>
                <td><span class="badge bg-warning text-white">::pickup</span> <span class="badge bg-info text-white">ItemID</span> <span class="badge bg-success text-white">number</span></td>
                <td>Gives player item + x amount</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandPickup">
                    Code
                  </button>
              </td>
              </tr>  

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Owner</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::ipban</span></th>
                <td><span class="badge bg-warning text-white">::ipban</span> <span class="badge bg-info text-white">PlayerName</span></td>
                <td>Bans player by IP</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandIPBan">
                    Code
                  </button>
              </td>
              </tr>  

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Staff</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::ban</span></th>
                <td><span class="badge bg-warning text-white">::ban</span> <span class="badge bg-info text-white">PlayerName</span></td>
                <td>Bans player by name</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandBanPlayer">
                    Code
                  </button>
              </td>
              </tr> 

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Staff</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::tele</span></th>
                <td><span class="badge bg-warning text-white">::tele</span> <span class="badge bg-info text-white">Coord X</span> <span class="badge bg-info text-white">Coord Y</span></td>
                <td>Teleports to X,Y position</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandTele">
                    Code
                  </button>
              </td>
              </tr>

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Staff</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::kick</span></th>
                <td><span class="badge bg-warning text-white">::kick</span> <span class="badge bg-info text-white">PlayerName</span> </td>
                <td>Kicks player by name</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandKick">
                    Code
                  </button>
              </td>
              </tr>

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Player</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::players</span></th>
                <td><span class="badge bg-warning text-white">::players</span> </td>
                <td>Displays number of players online</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandPlayers">
                    Code
                  </button>
              </td>
              </tr>

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Player</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::maxhit</span></th>
                <td><span class="badge bg-warning text-white">::maxhit</span> </td>
                <td>Displays player max hit</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandMaxHit">
                    Code
                  </button>
              </td>
              </tr>

              <tr>
                <th scope="row"><span class="badge bg-primary text-white">Player</span></th>
                <th scope="row"><span class="badge bg-warning text-white">::empty</span></th>
                <td><span class="badge bg-warning text-white">::empty</span> </td>
                <td>Empties player inventory</td>
                <td> 
                  <button type="button" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#CommandEmpty">
                    Code
                  </button>
              </td>
              </tr>  

            </tbody>
          </table>

          {% include 'commands/components/modals.twig' %}
          

          
        </div>
    </div>

    {{ content|raw }}
</div>

{%  endblock %}